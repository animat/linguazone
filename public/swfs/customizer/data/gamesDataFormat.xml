<?xml version="1.0" encoding="utf-8" ?>
<gamesDataFormat>
	
	<game name="alphabetTree">
		<format max="15" />
		<emptyquestion>
			<node>
				<question name="value" content="" type="" />
				<responses answer="all">
					<response name="decoy" content="" type="" />
				</responses>
			</node>
		</emptyquestion>
		<templateformat>
			<set label="theme" linkedto="correctGroupName">
				<option name="value" type="text" content="" />
			</set>
			<set label="This question is..." linkedto="decoy" locked="true">
				<option name="value" type="text" content="correct" />
				<option name="value" type="text" content="incorrect" />
			</set>
		</templateformat>
	</game>
	
	<game name="bathTubBubbles">
		<format max="25" />
		<emptyquestion>
			<node>
				<question name="value" content="" type="" />
				<responses answer="all">
					<response name="answer" content="" type="" />
				</responses>
			</node>
		</emptyquestion>
		<templateformat>
			<set label="Options" linkedto="answer" min="2" max="8">
				<option name="value" content="" type="" />
			</set>
		</templateformat>
	</game>
	
	<game name="cliffhanger">
		<format max="25" />
		<emptyquestion>
			<node>
				<question name="question" content="" type="" />
				<responses answer="all">
					<response name="ltarget" content="" type="" />
					<response name="rtarget" content="" type="" />
				</responses>
			</node>
		</emptyquestion>
		<templateformat>
			<set label="First match" linkedto="ltarget">
				<option name="value" content="" type="" />
			</set>
			<set label="Second match" linkedto="rtarget">
				<option name="value" content="" type="" />
			</set>
		</templateformat>
	</game>
	
