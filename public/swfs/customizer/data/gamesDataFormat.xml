	<game name="quizShow">
		<format max="25" />
		<emptyquestion>
			<node value="">
				<answers>
					<answer>
						<node value="" decoy="" />
					</answer>
					<answer>
						<node value="" decoy="" />
					</answer>
					<answer>
						<node value="" decoy="" />
					</answer>
					<answer>
						<node value="" decoy="" />
					</answer>
				</answers>
			</node>
		</emptyquestion>
	</game>
	
