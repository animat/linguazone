@media print{
	#instructions, h1{
		display:none;
	}
	table{
		clear:both;
		width:100%;
		border:1px solid #000;
		border-width:1px 0;
		page-break-after:auto;
	}
	table#last{
		page-break-after:auto;
	}
	td{
		text-align:center;
		font:12pt 'Verdana','Century Gothic','Lucida Grande',arial,sans-serif;
		border-bottom:1px dashed #bbb;
		height:1.75in;
		width:50%;
		padding-left:15px;
	}
	.left{
		border-right:1px solid #bbb;
		padding:0 15px 0 0;
	}
	.last td{
		border-bottom:none;
	}
}