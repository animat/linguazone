<% form_for @high_score do |f| %>
	<%= f.error_messages %>
	<p><%= f.label :score %> <%= f.text_field :score %></p>
	<p><%= f.label :game_id %> <%= f.text_field :game_id %></p>
	<p><%= f.label :user_id %> <%= f.text_field :user_id %></p>
	<%= f.hidden_field :user_ip_address, :value => request.remote_ip %>
	<p><%= f.submit "Submit" %></p>
<% end %>